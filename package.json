{
  "name": "medrecv2",
  "version": "1.0.0",
  "description": "Medical Records Application with Blockchain Integration",
  "main": "dist/electron-main/src/main.js",
  "scripts": {
    "start:renderer": "webpack serve --mode development",
    "build:renderer": "webpack --mode production",
    "build:main": "tsc -p tsconfig.json",
    "build": "npm run build:renderer && npm run build:main",
    "start:electron": "electron .",
    "dev": "concurrently \"npm run start:renderer\" \"wait-on http://localhost:3000 && cross-env NODE_ENV=development electron .\"",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "electron",
    "react",
    "typescript",
    "medical-records",
    "blockchain"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.0",
    "@electron/rebuild": "^3.7.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.14",
    "babel-jest": "^29.7.0",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^35.1.2",
    "electron-builder": "^26.0.12",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "wait-on": "^8.0.3"
  },
  "dependencies": {
    "@babel/core": "^7.26.10",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.0.0",
    "@mui/material": "^7.0.0",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "dotenv": "^16.5.0",
    "html-webpack-plugin": "^5.6.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.5.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1"
  }
